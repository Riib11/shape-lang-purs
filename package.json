{
  "private": true,
  "devDependencies": {
    "parcel": "^2.3.2",
    "purescript": "0.14.5",
    "spago": "^0.20.7",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "test": "spago test --purs-args \"--censor-warnings --stash -g sourcemaps\" --source-maps",
    "build": "spago bundle-app --purs-args \"--censor-warnings --stash -g sourcemaps\" --main Main --to dist/app.js --source-maps",
    "bundle": "spago bundle-app --purs-args \"--censor-warnings --stash -g sourcemaps\" --main Main --to dist/app.js --source-maps",
    "serve": "cd dist/; parcel index.html"
  },
  "dependencies": {
    "purescript-psa": "^0.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
